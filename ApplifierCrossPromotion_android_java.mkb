#!/usr/bin/env mkb
# Builder mkb file for the java portion of the ApplifierCrossPromotion extension on android
platform JAVA

files
{
    (source/android)
    ApplifierCrossPromotion.java
    (source/android/com/applifier/android/discovery)
    ApplifierView.java
    ApplifierViewListener.java
    ApplifierFullscreenActivity.java
    Unity3DHelper.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/ApplifierCrossPromotion.jar
