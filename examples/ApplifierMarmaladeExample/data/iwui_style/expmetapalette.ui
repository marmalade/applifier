CIwUIElement
{
	name ExpMetaPalette:s/Spacer

	CIwUIElement
	{
		name			Spacer
		
		CIwUILayout
		{
			element {
				CIwUILayoutSpacer {
					name "Spacer"
					sizeMin  { 30 30 }
					sizeMax  { 30 30 }
					sizeHint  { 30 30 }
				}
			}
		}
	}
}
