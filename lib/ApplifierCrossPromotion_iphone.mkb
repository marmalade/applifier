platform IPHONE

options
{
    lib
    output-name="lib/iphone/libApplifierCrossPromotion.a"
    output-name-debug="lib/iphone/libApplifierCrossPromotion_d.a"
}
define S3E_BUILD_EXT

subproject ApplifierCrossPromotion_build
